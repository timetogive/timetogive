{
  "cli": {
    "version": ">= 3.5.2"
  },
  "build": {
    "dev-client-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true
      },
      "env": {
        "APP_ENV": "dev"
      }
    },
    "dev-client-device": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "dev"
      }
    },
    "dev": {
      "channel": "dev",
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "APP_ENV": "dev"
      }
    },
    "prod": {
      "channel": "prod",
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
